pragma solidity ^0.8.17;

contract Counter {
    uint public count;

    function setCounter(uint _value) public {
        count = _value;
    }

    function incrementCounter() public {
        count++;
    }

    function decrementCounter() public {
        count--;
    }

    function getCounter() public view returns (uint) {
        return count;
    }
}
